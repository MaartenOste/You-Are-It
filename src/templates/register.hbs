<div class="o-login">
  <div class="m-username">
    <div class="a-label">
      <label for="uname">Email</label>
    </div>
    <input type="text" placeholder="Enter email" name="uname" id="emailfield">
  </div>

  <div class="m-password">
    <div class="a-label">
      <label for="psw">Password</label>
    </div>
    <input type="password" placeholder="Enter Password" name="psw" id="pwfield">
  </div>

  <div class="m-password">
    <div class="a-label">
      <label for="psw">Repeat password</label>
    </div>
    <input type="password" placeholder="Enter Password" name="psw" id="pwfield1">
  </div>

  {{routerLink "mainmenu" "register" "a-button_menu"}}
  {{routerLink "login" "back" "a-button_menu"}}
</div>
